<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ListarMascotasController</servlet-name>
		<display-name>ListarMascotasController</display-name>
		<description></description>
		<servlet-class>mascotinder.controladores.ListarMascotasController</servlet-class>

	</servlet>
	<servlet>
		<servlet-name>IngresarSistemaController</servlet-name>
		<display-name>IngresarSistemaController</display-name>
		<description></description>
		<servlet-class>mascotinder.controladores.IngresarSistemaController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InsertarMascotaController</servlet-name>
		<display-name>InsertarMascotaController</display-name>
		<description></description>
		<servlet-class>mascotinder.controladores.InsertarMascotaController</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>BuscarParejaController</servlet-name>
		<display-name>BuscarParejaController</display-name>
		<description></description>
		<servlet-class>mascotinder.controladores.BuscarParejaController</servlet-class>
	</servlet>	
	
	<servlet-mapping>
		<servlet-name>ListarMascotasController</servlet-name>
		<url-pattern>/ListarMascotasController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IngresarSistemaController</servlet-name>
		<url-pattern>/IngresarSistemaController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InsertarMascotaController</servlet-name>
		<url-pattern>/InsertarMascotaController</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>BuscarParejaController</servlet-name>
		<url-pattern>/BuscarParejaController</url-pattern>
	</servlet-mapping>	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
